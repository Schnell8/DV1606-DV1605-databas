<% res = res[0] %>
<form class="label-left" method="post" action="/eshop/order/create">
    <fieldset>

        <label for="kundnummer">Kundnummer</label>
        <input id="kundnummer" type="text" name="kundnummer" readonly value="<%= res.kundnummer %>">

        <label for="namn">Namm</label>
        <input id="namn" type="text" name="namn" readonly value="<%= res.namn %>">

        <label for="stad">Stad</label>
        <input id="stad" type="text" name="stad" readonly value="<%= res.stad %>">

        <label for="telefon">Telefon</label>
